{
  "name": "untouchable",
  "version": "0.2.1",
  "author": {
    "name": "Jinyong Park",
    "email": "dev.jinyongp@gmail.com"
  },
  "keywords": [
    "monkeypatch",
    "undetectable",
    "proxy"
  ],
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "readme": "README.md",
  "repository": "github:jinyongp/untouchable",
  "bugs": {
    "url": "https://github.com/jinyongp/untouchable/issues",
    "email": "dev.jinyongp@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "test:unit": "jest",
    "prepack": "npm run build",
    "build": "rimraf dist && npm-run-all -p build:*",
    "build:esm": "esbuild src/index.ts --outfile=dist/index.mjs --format=esm --target=es6",
    "build:cjs": "esbuild src/index.ts --outfile=dist/index.cjs --format=cjs --target=es6",
    "build:iife": "esbuild src/index.ts --outfile=dist/index.js --format=iife --target=es6 --global-name=untouchable",
    "build:types": "tsc src/index.ts --declaration --emitDeclarationOnly --outDir dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "esbuild": "^0.17.15",
    "is-proxy": "^1.0.6",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
